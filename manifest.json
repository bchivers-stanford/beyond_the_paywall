{
  "description": "Local News Demand Study",
  "author": "Stanford GSB",
  "manifest_version": 2,
  "name": "Local News Demand Study",
  "version": "0.1",
  "homepage_url": "https://github.com/bchivers-stanford/local_news_demand",
  
    
  "browser_specific_settings": {
    "gecko": {
      "id": "local-news-demand@rally.mozilla.org",
      "strict_min_version": "87.0"
    }
  },

  "permissions": [
    "<all_urls>",
    "idle",
    "notifications",
    "storage",
    "unlimitedStorage",
    "telemetry",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "icons": {
    "38": "icon.png"
  },
  "browser_action": {},
  "background": {
    "scripts": ["dist/background.js"]
  }
}
